# baseURI: http://onto.fel.cvut.cz/ontologies/test/apply-construct-with-chunked-values/spin-query-config

@prefix : <http://onto.fel.cvut.cz/ontologies/test/apply-construct-with-chunked-values/> .
@prefix kbss-module: <http://onto.fel.cvut.cz/ontologies/lib/module/> .
@prefix km-ac-wcv: <http://onto.fel.cvut.cz/ontologies/lib/module/apply-construct-with-chunked-values/> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix spkpis: <http://onto.fel.cvut.cz/ontologies/safety-performance-kpis/> .
@prefix sm: <http://topbraid.org/sparqlmotion#> .
@prefix sml: <http://topbraid.org/sparqlmotionlib#> .
@prefix sp: <http://spinrdf.org/sp#> .

kbss-module:apply-construct-with-chunked-values rdf:type sm:Module.

:construct-test-data
  a kbss-module:apply-construct-with-chunked-values ;
  km-ac-wcv:chunk-size 10000 ;
  sml:selectQuery [
      a sp:Select ;
      sp:text """# retrieve ordered timepoints of all trails

SELECT ?airport ?city
WHERE {
  VALUES (?airport ?city) {
    ( "CHO" "Charlottesville" )
    ( "RIC" "Richmond" )
    ( "LYH" "Lynchburg" )
  }
}
""" ;
    ] ;
  sml:constructQuery [
      a sp:Construct ;
      sp:text """# 1 - construct :has-next-trail-point relation
PREFIX spkpis: <http://onto.fel.cvut.cz/ontologies/safety-performance-kpis/> # TODO support prefixes

CONSTRUCT {
  ?timePoint__previous spkpis:has-next-trail-point ?timePoint .
} WHERE {

  #${VALUES}

  FILTER(?trail__previous = ?trail)
}""" ;
    ] ;
  sml:replace true ;
.

