@prefix kbss-module: <http://onto.fel.cvut.cz/ontologies/lib/module/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix sm: <http://topbraid.org/sparqlmotion#> .
@prefix sml: <http://topbraid.org/sparqlmotionlib#> .
@prefix sp: <http://spinrdf.org/sp#> .
@prefix spin: <http://spinrdf.org/spin#> .
@prefix tarql-module: <http://onto.fel.cvut.cz/ontologies/test/tarql-module#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

tarql-module:CreateSampleTriples
  rdf:type kbss-module:tarql ;
  sm:nodeX 292 ;
  sm:nodeY 155 ;
  sml:constructQuery [
      rdf:type sp:Construct ;
      sp:templates (
          [
            sp:object "example" ;
            sp:predicate rdfs:label ;
            sp:subject [
                sp:varName "ECCAIRSEventID"^^xsd:string ;
              ] ;
          ]
          [
            sp:object "some uri" ;
            sp:predicate rdfs:label ;
            sp:subject [
                sp:varName "URI"^^xsd:string ;
              ] ;
          ]
          [
            sp:object "some uri2" ;
            sp:predicate rdfs:label ;
            sp:subject [
                sp:varName "URI2"^^xsd:string ;
              ] ;
          ]
        ) ;
      sp:text """CONSTRUCT {
    ?ECCAIRSEventID rdfs:label \"example\" . 
    ?URI rdfs:label \"some uri\" .   
    ?URI2 rdfs:label \"some uri2\" .   
}
WHERE {
    BIND (URI(CONCAT('http://companies/', STR(?ROWNUM))) AS ?URI)
    BIND (URI(CONCAT('http://companies/', ?ECCAIRSEventID, \"---\",STR(?ROWNUM))) AS ?URI2)
}"""^^xsd:string ;
      sp:where (
          [
            rdf:type sp:Bind ;
            sp:expression [
                rdf:type sp:uri ;
                sp:arg1 [
                    rdf:type sp:concat ;
                    sp:arg1 "http://companies/" ;
                    sp:arg2 [
                        rdf:type sp:str ;
                        sp:arg1 [
                            sp:varName "ROWNUM"^^xsd:string ;
                          ] ;
                      ] ;
                  ] ;
              ] ;
            sp:variable [
                sp:varName "URI"^^xsd:string ;
              ] ;
          ]
          [
            rdf:type sp:Bind ;
            sp:expression [
                rdf:type sp:uri ;
                sp:arg1 [
                    rdf:type sp:concat ;
                    sp:arg1 "http://companies/" ;
                    sp:arg2 [
                        sp:varName "ECCAIRSEventID"^^xsd:string ;
                      ] ;
                    sp:arg3 "---" ;
                    sp:arg4 [
                        rdf:type sp:str ;
                        sp:arg1 [
                            sp:varName "ROWNUM"^^xsd:string ;
                          ] ;
                      ] ;
                  ] ;
              ] ;
            sp:variable [
                sp:varName "URI2"^^xsd:string ;
              ] ;
          ]
        ) ;
    ] ;
  sml:replace "true"^^xsd:boolean ;
  sml:sourceFilePath "/home/blcha/projects/kbss/git/s-pipes/s-pipes-core/src/test/resources/tarql-module/table.csv"^^xsd:string ;
  rdfs:label "Create sample triples"^^xsd:string ;
.
<http://onto.fel.cvut.cz/ontologies/test/tarql-module/spin-query-config>
  rdf:type owl:Ontology ;
  owl:imports <http://onto.fel.cvut.cz/ontologies/s-pipes> ;
  owl:imports <http://onto.fel.cvut.cz/ontologies/s-pipes-lib> ;
.
