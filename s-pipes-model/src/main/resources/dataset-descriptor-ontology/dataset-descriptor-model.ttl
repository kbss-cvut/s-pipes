###  http://onto.fel.cvut.cz/ontologies/dataset-descriptor/inv.has-source
ddo:inv.has-source rdf:type owl:ObjectProperty ;
                   rdfs:domain ddo:single-snapshot-dataset-source ;
                   rdfs:range ddo:dataset-publication ,
                              ddo:description ;
                   rdfs:label "INV.has source" .


###  http://onto.fel.cvut.cz/ontologies/dataset-descriptor/inv.has-sub-dataset-snapshot
ddo:inv.has-sub-dataset-snapshot rdf:type owl:ObjectProperty ;
                                 rdfs:domain ddo:dataset-snapshot ;
                                 rdfs:range ddo:dataset-snapshot ;
                                 rdfs:label "INV.has sub dataset snapshot" .


###  http://onto.fel.cvut.cz/ontologies/dataset-descriptor/inv.has-subdataset
ddo:inv.has-subdataset rdf:type owl:ObjectProperty ;
                       rdfs:domain ddo:dataset ;
                       rdfs:range ddo:dataset ;
                       rdfs:label "INV.has subdataset" .


###  http://onto.fel.cvut.cz/ontologies/dataset-descriptor/inv.is-created-by
ddo:inv.is-created-by rdf:type owl:ObjectProperty ;
                      owl:inverseOf ddo:is-created-by ;
                      rdfs:domain ddo:transformation ;
                      rdfs:range ddo:execution-context-dataset-source ;
                      rdfs:label "INV.is created by" .


###  http://onto.fel.cvut.cz/ontologies/dataset-descriptor/inv.is-description-of
ddo:inv.is-description-of rdf:type owl:ObjectProperty ;
                          owl:inverseOf ddo:is-description-of ;
                          rdfs:domain ddo:described-data-artifact ;
                          rdfs:range ddo:description ;
                          rdfs:label "INV.is description of" .


###  http://onto.fel.cvut.cz/ontologies/dataset-descriptor/inv.is-part-of
ddo:inv.is-part-of rdf:type owl:ObjectProperty ;
                   owl:inverseOf ddo:is-part-of ;
                   rdfs:domain ddo:data ;
                   rdfs:range ddo:data ;
                   rdfs:label "INV.is part of" .


###  http://onto.fel.cvut.cz/ontologies/dataset-descriptor/inv.offers-dataset
ddo:inv.offers-dataset rdf:type owl:ObjectProperty ;
