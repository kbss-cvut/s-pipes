             rdfs:range ddo:dataset-exploration ;
             rdfs:label "INV.uses" .


###  http://onto.fel.cvut.cz/ontologies/dataset-descriptor/is-created-by
ddo:is-created-by rdf:type owl:ObjectProperty ;
                  rdfs:domain ddo:execution-context-dataset-source ;
                  rdfs:range ddo:transformation ;
