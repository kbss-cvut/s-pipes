ddo:has-retrieval-date rdf:type owl:DatatypeProperty ;
                       rdfs:domain ddo:dataset-snapshot ;
                       rdfs:range xsd:string ;
                       rdfs:label "has-retrieval-date" .


###  http://onto.fel.cvut.cz/ontologies/dataset-descriptor/name
ddo:name rdf:type owl:DatatypeProperty ;
         rdfs:domain ddo:dataset-source ;
         rdfs:range xsd:string ;
         rdfs:label "name" .


#################################################################
#    Classes
#################################################################

###  http://onto.fel.cvut.cz/ontologies/dataset-descriptor/agent
ddo:agent rdf:type owl:Class ;
          rdfs:subClassOf owl:Thing ;
          rdfs:label "Agent" .


###  http://onto.fel.cvut.cz/ontologies/dataset-descriptor/content-descriptor
ddo:content-descriptor rdf:type owl:Class ;
                       rdfs:subClassOf ddo:dataset-descriptor ;
                       rdfs:label "Content Descriptor" .


###  http://onto.fel.cvut.cz/ontologies/dataset-descriptor/data
ddo:data rdf:type owl:Class ;
         rdfs:subClassOf owl:Thing ,
                         [ rdf:type owl:Restriction ;
                           owl:onProperty ddo:inv.is-part-of ;
                           owl:allValuesFrom ddo:data
                         ] ,
                         [ rdf:type owl:Restriction ;
                           owl:onProperty ddo:has-member ;
                           owl:minQualifiedCardinality "0"^^xsd:nonNegativeInteger ;
                           owl:onClass ddo:data-item
                         ] ,
                         [ rdf:type owl:Restriction ;
                           owl:onProperty ddo:inv.constitutes ;
                           owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                           owl:onClass ddo:dataset-snapshot
                         ] ,
                         [ rdf:type owl:Restriction ;
                           owl:onProperty ddo:is-part-of ;
                           owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                           owl:onClass ddo:data
                         ] ;
         rdfs:label "Data" .


###  http://onto.fel.cvut.cz/ontologies/dataset-descriptor/data-item
ddo:data-item rdf:type owl:Class ;
              rdfs:subClassOf owl:Thing ,
                              [ rdf:type owl:Restriction ;
                                owl:onProperty ddo:inv.has-item ;
                                owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                owl:onClass ddo:single-item-selector
                              ] ,
                              [ rdf:type owl:Restriction ;
                                owl:onProperty ddo:inv.has-member ;
                                owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                owl:onClass ddo:data
                              ] ;
              rdfs:label "Data Item" .


###  http://onto.fel.cvut.cz/ontologies/dataset-descriptor/dataset
ddo:dataset rdf:type owl:Class ;
            rdfs:subClassOf owl:Thing ,
                            [ rdf:type owl:Restriction ;
                              owl:onProperty ddo:inv.has-dataset ;
                              owl:allValuesFrom ddo:dataset-snapshot
                            ] ,
                            [ rdf:type owl:Restriction ;
                              owl:onProperty ddo:inv.has-explored-dataset ;
                              owl:allValuesFrom ddo:dataset-exploration
                            ] ,
                            [ rdf:type owl:Restriction ;
                              owl:onProperty ddo:inv.offers-dataset ;
                              owl:allValuesFrom ddo:dataset-source
                            ] ,
                            [ rdf:type owl:Restriction ;
                              owl:onProperty ddo:has-subdataset ;
                              owl:minQualifiedCardinality "0"^^xsd:nonNegativeInteger ;
                              owl:onClass ddo:dataset
                            ] ,
                            [ rdf:type owl:Restriction ;
                              owl:onProperty ddo:inv.has-subdataset ;
                              owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                              owl:onClass ddo:dataset
                            ] ;
            rdfs:label "Dataset" .


###  http://onto.fel.cvut.cz/ontologies/dataset-descriptor/dataset-descriptor
ddo:dataset-descriptor rdf:type owl:Class ;
                       rdfs:subClassOf ddo:dataset-snapshot ,
                                       [ rdf:type owl:Restriction ;
                                         owl:onProperty ddo:describes ;
                                         owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                         owl:onClass ddo:described-data-artifact
                                       ] ,
                                       [ rdf:type owl:Restriction ;
                                         owl:onProperty ddo:inv.has-dataset-descriptor ;
                                         owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                         owl:onClass ddo:description
                                       ] ;
                       rdfs:label "Dataset Descriptor" .


###  http://onto.fel.cvut.cz/ontologies/dataset-descriptor/dataset-exploration
ddo:dataset-exploration rdf:type owl:Class ;
                        rdfs:subClassOf owl:Thing ,
                                        [ rdf:type owl:Restriction ;
                                          owl:onProperty ddo:has-explored-dataset ;
                                          owl:allValuesFrom ddo:dataset
                                        ] ,
                                        [ rdf:type owl:Restriction ;
                                          owl:onProperty ddo:uses ;
                                          owl:allValuesFrom ddo:intent
                                        ] ,
                                        [ rdf:type owl:Restriction ;
                                          owl:onProperty ddo:has-dataset-explorer ;
                                          owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                          owl:onClass ddo:dataset-exploring-journalist
                                        ] ;
                        rdfs:label "Dataset Exploration" .


###  http://onto.fel.cvut.cz/ontologies/dataset-descriptor/dataset-exploring-journalist
ddo:dataset-exploring-journalist rdf:type owl:Class ;
                                 rdfs:subClassOf ddo:person ,
                                                 [ rdf:type owl:Restriction ;
                                                   owl:onProperty ddo:has-intent ;
                                                   owl:allValuesFrom ddo:intent
                                                 ] ,
                                                 [ rdf:type owl:Restriction ;
                                                   owl:onProperty ddo:inv.has-dataset-explorer ;
                                                   owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                                   owl:onClass ddo:dataset-exploration
                                                 ] ;
                                 rdfs:label "Dataset Exploring Journalist" .


###  http://onto.fel.cvut.cz/ontologies/dataset-descriptor/dataset-publication
ddo:dataset-publication rdf:type owl:Class ;
                        rdfs:subClassOf owl:Thing ,
                                        [ rdf:type owl:Restriction ;
                                          owl:onProperty ddo:has-source ;
                                          owl:allValuesFrom ddo:single-snapshot-dataset-source
                                        ] ,
                                        [ rdf:type owl:Restriction ;
                                          owl:onProperty ddo:has-published-dataset-snapshot ;
                                          owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                          owl:onClass ddo:dataset-snapshot
                                        ] ,
                                        [ rdf:type owl:Restriction ;
                                          owl:onProperty ddo:has-publisher ;
                                          owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                          owl:onClass ddo:publisher
                                        ] ;
                        rdfs:label "Dataset Publication" .


###  http://onto.fel.cvut.cz/ontologies/dataset-descriptor/dataset-selector
ddo:dataset-selector rdf:type owl:Class ;
                     rdfs:subClassOf ddo:content-descriptor ;
                     rdfs:label "Dataset Selector" .


###  http://onto.fel.cvut.cz/ontologies/dataset-descriptor/dataset-snapshot
ddo:dataset-snapshot rdf:type owl:Class ;
                     rdfs:subClassOf owl:Thing ,
                                     [ rdf:type owl:Restriction ;
                                       owl:onProperty ddo:constitutes ;
                                       owl:allValuesFrom ddo:data
                                     ] ,
                                     [ rdf:type owl:Restriction ;
                                       owl:onProperty ddo:has-sub-dataset-snapshot ;
                                       owl:minQualifiedCardinality "0"^^xsd:nonNegativeInteger ;
                                       owl:onClass ddo:dataset-snapshot
                                     ] ,
                                     [ rdf:type owl:Restriction ;
                                       owl:onProperty ddo:has-dataset ;
                                       owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                       owl:onClass ddo:dataset
                                     ] ,
                                     [ rdf:type owl:Restriction ;
                                       owl:onProperty ddo:inv.has-published-dataset-snapshot ;
                                       owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                       owl:onClass ddo:dataset-publication
                                     ] ,
                                     [ rdf:type owl:Restriction ;
                                       owl:onProperty ddo:inv.has-sub-dataset-snapshot ;
                                       owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                       owl:onClass ddo:dataset-snapshot
                                     ] ;
                     rdfs:label "Dataset Snapshot" .


###  http://onto.fel.cvut.cz/ontologies/dataset-descriptor/dataset-source
ddo:dataset-source rdf:type owl:Class ;
                   rdfs:subClassOf owl:Thing ,
                                   [ rdf:type owl:Restriction ;
                                     owl:onProperty ddo:offers-dataset ;
                                     owl:allValuesFrom ddo:dataset
                                   ] ;
                   rdfs:label "Dataset Source" .


###  http://onto.fel.cvut.cz/ontologies/dataset-descriptor/described-data-artifact
ddo:described-data-artifact rdf:type owl:Class ;
                            rdfs:subClassOf owl:Thing ,
                                            [ rdf:type owl:Restriction ;
                                              owl:onProperty ddo:inv.describes ;
                                              owl:allValuesFrom ddo:dataset-descriptor
                                            ] ,
                                            [ rdf:type owl:Restriction ;
                                              owl:onProperty ddo:inv.is-description-of ;
                                              owl:allValuesFrom ddo:description
                                            ] ;
                            rdfs:label "Described Data Artifact" .


###  http://onto.fel.cvut.cz/ontologies/dataset-descriptor/described-dataset
ddo:described-dataset rdf:type owl:Class ;
                      rdfs:subClassOf ddo:dataset ,
                                      ddo:described-data-artifact ;
                      rdfs:label "Described Dataset" .


###  http://onto.fel.cvut.cz/ontologies/dataset-descriptor/described-dataset-snapshot
ddo:described-dataset-snapshot rdf:type owl:Class ;
                               rdfs:subClassOf ddo:dataset-snapshot ,
                                               ddo:described-data-artifact ;
                               rdfs:label "Described Dataset Snapshot" .


###  http://onto.fel.cvut.cz/ontologies/dataset-descriptor/description
