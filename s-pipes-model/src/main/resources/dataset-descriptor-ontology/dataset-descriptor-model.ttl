@prefix : <http://onto.fel.cvut.cz/ontologies/dataset-descriptor#> .
@prefix ddo: <http://onto.fel.cvut.cz/ontologies/dataset-descriptor/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@base <http://onto.fel.cvut.cz/ontologies/dataset-descriptor> .

<http://onto.fel.cvut.cz/ontologies/dataset-descriptor> rdf:type owl:Ontology .

#################################################################
#    Object Properties
#################################################################

###  http://onto.fel.cvut.cz/ontologies/dataset-descriptor/constitutes
ddo:constitutes rdf:type owl:ObjectProperty ;
                owl:inverseOf ddo:inv.constitutes ;
                rdfs:domain ddo:dataset-snapshot ;
                rdfs:range ddo:data ;
                rdfs:label "constitutes" .


###  http://onto.fel.cvut.cz/ontologies/dataset-descriptor/describes
ddo:describes rdf:type owl:ObjectProperty ;
              owl:inverseOf ddo:inv.describes ;
              rdfs:domain ddo:dataset-descriptor ;
              rdfs:range ddo:described-data-artifact ;
              rdfs:label "describes" .


