ddo:uses rdf:type owl:ObjectProperty ;
         rdfs:domain ddo:dataset-exploration ;
         rdfs:range ddo:intent ;
         rdfs:label "uses" .


#################################################################
#    Data properties
#################################################################

###  http://onto.fel.cvut.cz/ontologies/dataset-descriptor/has-creation-date
ddo:has-creation-date rdf:type owl:DatatypeProperty ;
                      rdfs:domain ddo:dataset-publication ,
                                  ddo:description ;
                      rdfs:range xsd:string ;
                      rdfs:label "has creation date" .


###  http://onto.fel.cvut.cz/ontologies/dataset-descriptor/has-data-hash
ddo:has-data-hash rdf:type owl:DatatypeProperty ;
                  rdfs:domain ddo:dataset-snapshot ;
                  rdfs:range xsd:string ;
                  rdfs:label "has-data-hash" .


###  http://onto.fel.cvut.cz/ontologies/dataset-descriptor/has-download-url
ddo:has-download-url rdf:type owl:DatatypeProperty ;
                     rdfs:domain ddo:url-dataset-source ;
                     rdfs:range xsd:string ;
                     rdfs:label "has download url" .


###  http://onto.fel.cvut.cz/ontologies/dataset-descriptor/has-endpoint-url
ddo:has-endpoint-url rdf:type owl:DatatypeProperty ;
                     rdfs:domain ddo:named-graph-sparql-endpoint-dataset-source ,
                                 ddo:sparql-endpoint-dataset-source ;
                     rdfs:range xsd:string ;
                     rdfs:label "has endpoint url" .


###  http://onto.fel.cvut.cz/ontologies/dataset-descriptor/has-graph-id
ddo:has-graph-id rdf:type owl:DatatypeProperty ;
                 rdfs:domain ddo:named-graph-sparql-endpoint-dataset-source ;
                 rdfs:range xsd:string ;
                 rdfs:label "has graph id" .


