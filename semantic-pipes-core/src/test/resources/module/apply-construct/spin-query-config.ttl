@prefix apply-construct: <http://onto.fel.cvut.cz/ontologies/test/apply-construct#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix sm: <http://topbraid.org/sparqlmotion#> .
@prefix sml: <http://topbraid.org/sparqlmotionlib#> .
@prefix sp: <http://spinrdf.org/sp#> .
@prefix spin: <http://spinrdf.org/spin#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

apply-construct:CreateSampleTriples
  rdf:type sml:ApplyConstruct ;
  sm:nodeX 292 ;
  sm:nodeY 155 ;
  sml:constructQuery [
      rdf:type sp:Construct ;
      sp:templates (
          [
            sp:object "example" ;
            sp:predicate rdfs:label ;
            sp:subject <http://example.org> ;
          ]
        ) ;
      sp:text """CONSTRUCT {
    <http://example.org> rdfs:label \"example\" .  
}
WHERE {
    
}"""^^xsd:string ;
      sp:where () ;
    ] ;
  sml:constructQuery [
      rdf:type sp:Construct ;
      sp:templates (
          [
            sp:object "sample" ;
            sp:predicate rdfs:label ;
            sp:subject <http://example.org> ;
          ]
        ) ;
      sp:text """CONSTRUCT {
       <http://example.org> rdfs:label \"sample\" .    
}
WHERE {
    
}"""^^xsd:string ;
      sp:where () ;
    ] ;
  sml:replace "true"^^xsd:boolean ;
  rdfs:label "Create sample triples"^^xsd:string ;
.
<http://onto.fel.cvut.cz/ontologies/test/apply-construct/spin-query-config>
  rdf:type owl:Ontology ;
  owl:imports <http://topbraid.org/sparqlmotionfunctions> ;
  owl:imports <http://topbraid.org/sparqlmotionlib-tbc> ;
  owl:versionInfo "Created with TopBraid Composer"^^xsd:string ;
.
